{
  "parser": "babel-eslint",
  "globals": {
      "console": true,
      "_": true,
      "$"
  },
  "rules": {
      "indent": [
          2,
          2
      ],
      "quotes": [
          2,
          "single"
      ],
      "linebreak-style": [
          2,
          "unix"
      ],
      "semi": [
          0,
          "always"
      ],
      "no-extra-semi": 0,
      "no-console": 0
  },
  "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
  },
  "plugins": [
      "react"
  ]
}
